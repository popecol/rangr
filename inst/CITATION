citHeader("To cite rangr in publications use:")

bibentry(
  key = "rangr",
  bibtype = "manual",
  title = "rangr: simulating range dynamics of virtual species",
  author = c(
    person("Katarzyna", "Markowska"),
    person("Lechosław", "Kuczyński")),
  year = "2023",
  note = "R package version 1.0.5",
  url = "https://github.com/ropensci/rangr"
)
